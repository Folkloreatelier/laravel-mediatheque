{
    "name": "folklore/laravel-mediatheque",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/Folkloreatelier",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@atelierfolklore.ca",
            "homepage": "http://atelierfolklore.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "guzzlehttp/guzzle": "~6.0|^6.3",
        "folklore/image": "~1.0.0-beta.2",
        "php-ffmpeg/php-ffmpeg": "^0.14.0",
        "symfony/http-foundation": "~3.3|~4.0|^4.1|^4.2",
        "symfony/process": "~3.3|~4.0|^4.1|^4.2",
        "ramsey/uuid": "^3.7"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/database": "3.5.*|3.6.*|3.7.*|3.8.*",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.0|~7.0|^7.5",
        "doctrine/dbal": "^2.5|^2.6",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/PHP-FFMpeg/BinaryDriver.git"
        }
    ],
    "suggests": {
        "ext-imagick": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "classmap": [
            "tests/"
        ],
        "psr-0": {
            "Folklore\\Mediatheque": "src/"
        },
        "files": [
            "src/Folklore/Mediatheque/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\Mediatheque\\MediathequeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
