            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "guzzlehttp/guzzle": "5.3|~6.0",
        "folklore/image": "v1.x-dev",
        "php-ffmpeg/php-ffmpeg": "^0.13.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/database": "3.1.*|3.2.*|3.3.*|3.4.*|3.5.*|3.6.*|3.7.*",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*|3.5.*|3.6.*|3.7.*",
        "mockery/mockery": "0.9.*|1.0.*",
        "phpunit/phpunit": "~4.0|~5.0|~5.7|~6.0|~7.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "suggests": {
        "ext-imagick": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "classmap": [
            "tests/"
        ],
        "psr-0": {
            "Folklore\\Mediatheque": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\Mediatheque\\MediathequeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
